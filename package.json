{
  "name": "action-mirror-action",
  "version": "1.0.0",
  "description": "Easily mirror actions from GitHub.com or other Git repositories to your own GitHub enterprise instance.",
  "main": "lib/main.js",
  "author": "Alexej Disterhoft <github@disterhoft.de>",
  "repository": "https://github.com/nobbs/action-mirror-action",
  "license": "MIT",
  "private": true,
  "scripts": {
    "build": "tsc",
    "format": "prettier --write '**/*.ts'",
    "format-check": "prettier --check '**/*.ts'",
    "lint": "eslint src/**/*.ts",
    "ci": "yarn install --frozen-lockfile",
    "package": "ncc build --source-map --license licenses.txt",
    "test": "jest",
    "all": "yarn build && yarn format && yarn lint && yarn package && yarn test",
    "release": "semantic-release"
  },
  "dependencies": {
    "@actions/core": "^1.10.0",
    "@actions/exec": "^1.1.1",
    "@actions/github": "^5.1.1",
    "@octokit/rest": "^19.0.7",
    "yaml": "^2.2.1"
  },
  "devDependencies": {
    "@jest/globals": "^29.4.2",
    "@semantic-release/changelog": "^6.0.2",
    "@semantic-release/commit-analyzer": "^9.0.2",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^8.0.7",
    "@semantic-release/npm": "^9.0.2",
    "@semantic-release/release-notes-generator": "^10.0.3",
    "@types/node": "^18.15.5",
    "@typescript-eslint/eslint-plugin": "^5.56.0",
    "@typescript-eslint/parser": "^5.56.0",
    "@vercel/ncc": "^0.36.1",
    "eslint": "^8.34.0",
    "eslint-config-prettier": "^8.8.0",
    "jest": "^29.4.2",
    "prettier": "2.8.5",
    "semantic-release": "^20.1.3",
    "ts-jest": "^29.0.5",
    "typescript": "^5.0.2"
  }
}
